{
  "Authentication": {
		"Jwt": {
			"Key": "%{JwtKey}%",
			"Issuer": "%{JwtIssuer}%",
			"Audiences": [ "%{FrontendUrl}%", "%{BackendUrl}%", "%{BackendUrlSecure}%" ],
			"RefreshTokenExpiration": 24 // 24 hours
		},
		"Google": {
			"ClientId": "%{GoogleClientId}%",
			"ClientSecret": "%{GoogleClientSecret}%",
			"RedirectUri": "%{GoogleRedirectUri}%"
		}
  }
}